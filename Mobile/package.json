{
  "name": "react-native-boilerplate",
  "version": "2.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "ci": "watchman watch-del-all && rm -rf node_modules && npm install",
    "prestart": "watchman watch-del-all",
    "start": "NODE_ENV=production npx expo start -c",
    "start:ios": "expo start -c --tunnel",
    "start:dev": "NODE_ENV=development npx expo start -c",
    "android": "expo run:android",
    "android:dev": "NODE_ENV=development npx expo start --android",
    "ios": "expo run:ios",
    "ios:dev": "NODE_ENV=development npx expo start --ios",
    "web": "NODE_ENV=production npx expo start --web",
    "web:dev": "NODE_ENV=development npx expo start --web",
    "format": "prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --verbose --passWithNoTests",
    "test:watch": "jest --verbose --passWithNoTests --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.3",
    "@rneui/themed": "^4.0.0-rc.8",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "^52.0.0",
    "expo-asset": "~11.0.1",
    "expo-camera": "~16.0.7",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.2",
    "expo-image-picker": "~16.0.3",
    "expo-linear-gradient": "~14.0.1",
    "expo-media-library": "~17.0.3",
    "expo-permissions": "^14.4.0",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.9",
    "jwt-decode": "^4.0.0",
    "ky": "^1.7.2",
    "lodash": "^4.17.21",
    "native-base": "^3.4.28",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-camera": "^4.2.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-gifted-chat": "^2.6.4",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-vision-camera": "^4.5.3",
    "react-native-web": "~0.19.13",
    "react-redux": "^9.1.2",
    "react-toastify": "^10.0.5",
    "react-webcam": "^7.2.0",
    "redux-logger": "^3.0.6",
    "sonner": "^1.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
